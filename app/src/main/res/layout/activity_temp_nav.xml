<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    android:layout_margin="100dp"
    tools:context=".TempNavActivity">

    <Button
        android:id="@+id/btnSuper"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Super Admin" />

    <Button
        android:id="@+id/btnAirline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Airline Admin" />

    <Button
        android:id="@+id/btnHotel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hotel Admin" />

    <Button
        android:id="@+id/btnRegular"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Regular User" />
</LinearLayout>